{% extends 'WellnessCoreBundle::layout_backend.html.twig' %}

{% block breadcrumb %}
    <li><a href="#">{{ 'Liste des utilisateurs'|trans }}</a></li>
{% endblock %}

{% block title %}
    {{ parent() }}
{% endblock %}


{% block contentadmin %}
    <h1>User</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ entity.slug }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Telnumber</th>
                <td>{{ entity.telnumber }}</td>
            </tr>
            <tr>
                <th>Addressstreet</th>
                <td>{{ entity.addressstreet }}</td>
            </tr>
            <tr>
                <th>Addressnumber</th>
                <td>{{ entity.addressnumber }}</td>
            </tr>
            <tr>
                <th>Inscriptioncf</th>
                <td>{{ entity.inscriptioncf }}</td>
            </tr>
            <tr>
                <th>Usertype</th>
                <td>{{ entity.usertype }}</td>
            </tr>
            <tr>
                <th>Tryingnumber</th>
                <td>{{ entity.tryingnumber }}</td>
            </tr>
            <tr>
                <th>Bamed</th>
                <td>{{ entity.bamed }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_user') }}">
            <i class="icon-backward"> </i> {{ 'Retour Ã  la liste'|trans }}
        </a>
    </li>
    <li>
        <a href="{{ path('admin_user_edit', { 'id': entity.id }) }}">
            <i class="icon-edit"> </i> {{ 'Editer'|trans }}
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
